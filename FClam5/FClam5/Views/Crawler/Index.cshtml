@model IEnumerable<FClam5.Models.ErrorReport>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.reportNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.URL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.parentURL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.errorType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.reportNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.URL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.parentURL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.errorType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.errorNumber }) |
            @Html.ActionLink("Details", "Details", new { id=item.errorNumber }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.errorNumber })
        </td>
    </tr>
}

</table>
